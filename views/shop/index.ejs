											<h4 class="title-1 title-border text-uppercase mb-30">Logout customers</h4>
											<p class="text-gray">Are you sure signout ?</p>
											<form action="/logout" method="post">
												<input type="hidden" name="_csrf" value="<%= csrfToken %>">
												<button class="button-one submit-button mt-15" data-text="logout" type="submit">Logout</button>
											</form>
										</div>
									</li>
								<% } %>
								<li><a href="/my-account" title="My-Account"><i class="zmdi zmdi-account"></i></a></li>
								<li><a href="/wishlist" title="Wishlist"><i class="zmdi zmdi-favorite"></i></a></li>
							</ul>
						</div>
					</div>
				</div>
				<!-- Sidebar-social-media start -->
			</section>
			<!-- End Slider-section -->
			<!-- sidebar-search Start -->
			<div class="sidebar-search animated slideOutUp">
